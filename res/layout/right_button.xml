<?xml version="1.0" encoding="utf-8"?> 
<selector xmlns:android="http://schemas.android.com/apk/res/android"> 
    <item android:state_pressed="true"> 
        <shape> 
            <gradient android:startColor="#84ae00" android:endColor="#94cb10" 
                android:angle="90" /> 
            <stroke android:width="1dip" android:color="#0c5b9c" /> 
            <corners android:radius="3dp" /> 
        </shape> 
    </item> 
  
    <item android:state_focused="true"> 
        <shape> 
            <gradient android:startColor="#84ae00" android:endColor="#94cb10" 
                android:angle="90" /> 
            <stroke android:width="1dip" android:color="#0c5b9c" /> 
            <corners android:radius="3dp" /> 
        </shape> 
    </item> 
  
    <item> 
        <shape> 
            <gradient android:startColor="#94cb10" android:endColor="#84ae00" 
                android:angle="90" /> 
            <stroke android:width="1dip" android:color="#0c5b9c" /> 
            <corners android:radius="3dp" /> 

        </shape> 
    </item> 
</selector>  
